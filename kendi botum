import random  

def random_emoji():  
    emojis = ['ğŸ˜€', 'ğŸ˜‚', 'ğŸ˜', 'ğŸ¤”', 'ğŸ˜­', 'ğŸ˜', 'ğŸ˜¡', 'ğŸ¥³', 'ğŸ˜±', 'ğŸ˜´', 'ğŸŒˆ', 'ğŸ•', 'ğŸš€', 'ğŸ‰', 'ğŸ’–', 'ğŸŒ']  
    return random.choice(emojis)  

def double_emoji(s):  
    result = ''  
    for letter in s:  
        result += random_emoji()  # Rastgele emoji ekleme  
    return result  

def secret_emoji_function(a, b):  
    return random_emoji()  # Rastgele emoji dÃ¶ndÃ¼rme  

print(double_emoji("Hello"))  # "Hello" yerine rastgele emojiler dÃ¶ndÃ¼recek  
print(secret_emoji_function(1, 2))  # Rastgele bir emoji dÃ¶ndÃ¼recek  
print(secret_emoji_function("Hello, ", "world!"))  # Rastgele bir emoji dÃ¶ndÃ¼recek
